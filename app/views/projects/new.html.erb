<h1>New Project</h1>

<%= form_for :project, url: projects_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :image %><br>
    <%= f.text_field :image %>
  </p>

  <p>
    <%= f.label :demo_link %><br>
    <%= f.text_field :demo_link %>
  </p>

  <p>
    <%= f.label :github_link %><br>
    <%= f.text_field :github_link %>
  </p>

  <%= f.submit %>
<% end %>

<%= link_to 'Back', projects_path %>